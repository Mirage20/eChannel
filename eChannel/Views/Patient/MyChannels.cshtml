@if (ViewData["patient_channels"] != null)
{
    List<eChannel.Models.Channel> channels = (List<eChannel.Models.Channel>)ViewData["patient_channels"];

    <div class="col-lg-9">
        <table class="table table-striped table-hover ">
            <tbody>
                <tr>
                    <td><b>Channel ID</b></td>
                    <td><b>Channel Number</b></td>
                    <td><b>Reason</b></td>
                    
                </tr>
                @foreach (eChannel.Models.Channel channel in channels)
                {

                    <tr @if (channel.ChannelRating == 0) { Write("class=success"); }>

                        <td>@channel.ChannelID</td>
                        <td>@channel.ChannelNumber</td>
                        <td>@channel.Reason</td>
                        
                    </tr>
                }
            </tbody>
        </table>
    </div>
}

