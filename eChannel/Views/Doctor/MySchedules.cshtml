@if (ViewData["doctor_schedules"] != null)
{
    List<eChannel.Models.DoctorSchedule> schedules = (List<eChannel.Models.DoctorSchedule>)ViewData["doctor_schedules"];

    <div class="col-lg-9">
        <table class="table table-striped table-hover ">
            <tbody>
                <tr>
                    <td><b>Work ID</b></td>
                    <td><b>Hospital Name</b></td>
                    <td><b>Room Name</b></td>
                    <td><b>Start Time</b></td>
                    <td><b>End Time</b></td>
                    <td><b>Maximum Channels</b></td>
                    <td><b>Patients Applied</b></td>
                </tr>
                @foreach (eChannel.Models.DoctorSchedule schedule in schedules)
                {

                    <tr @if (schedule.EndDateTime.CompareTo(DateTime.Now) > 0) { Write("class=success"); }>

                        <td>@schedule.WorkID</td>
                        <td>@schedule.HospitalName</td>
                        <td>@schedule.RoomName</td>
                        <td>@schedule.StartDateTime.ToString("yyyy-MM-dd hh:mm:ss tt")</td>
                        <td>@schedule.EndDateTime.ToString("yyyy-MM-dd hh:mm:ss tt")</td>
                        <td>@schedule.MaxChannels</td>
                        <td>@schedule.PatientApplied</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
}

